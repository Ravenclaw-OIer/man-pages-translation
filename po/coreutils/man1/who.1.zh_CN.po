#, no-wrap
msgid "WHO"
msgstr "WHO"

#. type: TH
#: raw/coreutils/man1/who.1:2
#, no-wrap
msgid "September 2022"
msgstr "2022年9月"

#. type: TH
#: raw/coreutils/man1/who.1:2
#, no-wrap
#| msgid "GNU coreutils 8.32"
msgid "GNU coreutils 9.1"
msgstr "GNU coreutils 9.1"

#. type: TH
#: raw/coreutils/man1/who.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: raw/coreutils/man1/who.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man1/who.1:5
msgid "who - show who is logged on"
msgstr "who - 显示已登录用户"

#. type: SH
#: raw/coreutils/man1/who.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/who.1:8
msgid "B<who> [I<\\,OPTION\\/>]... [ I<\\,FILE | ARG1 ARG2 \\/>]"
msgstr "B<who> [I<\\,选项\\/>]... [ I<\\,文件 | 参数1 参数2 \\/>]"

#. type: SH
#: raw/coreutils/man1/who.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/who.1:12
msgid "Print information about users who are currently logged in."
msgstr "显示与当前登录用户有关的信息。"

#. type: TP
#: raw/coreutils/man1/who.1:12
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
#: raw/coreutils/man1/who.1:15
msgid "same as B<-b> B<-d> B<--login> B<-p> B<-r> B<-t> B<-T> B<-u>"
msgstr "等价于 B<-b> B<-d> B<--login> B<-p> B<-r> B<-t> B<-T> B<-u>"

#. type: TP
#: raw/coreutils/man1/who.1:15
#, no-wrap
msgid "B<-b>, B<--boot>"
msgstr "B<-b>, B<--boot>"

#. type: Plain text
#: raw/coreutils/man1/who.1:18
msgid "time of last system boot"
msgstr "最近一次系统引导的时间"

#. type: TP
#: raw/coreutils/man1/who.1:18
#, no-wrap
msgid "B<-d>, B<--dead>"
msgstr "B<-d>, B<--dead>"

#. type: Plain text
#: raw/coreutils/man1/who.1:21
msgid "print dead processes"
msgstr "显示死进程"

#. type: TP
#: raw/coreutils/man1/who.1:21
#, no-wrap
msgid "B<-H>, B<--heading>"
msgstr "B<-H>, B<--heading>"

#. type: Plain text
#: raw/coreutils/man1/who.1:24
msgid "print line of column headings"
msgstr "显示表格的表头"

#. type: TP
#: raw/coreutils/man1/who.1:24
#, no-wrap
msgid "B<--ips>"
msgstr "B<--ips>"

#. type: Plain text
#: raw/coreutils/man1/who.1:29
msgid ""
"print ips instead of hostnames. with B<--lookup>, canonicalizes based on "
"stored IP, if available, rather than stored hostname"
msgstr ""
"显示 IP地址而不是主机名。加上 B<--lookup>，则会尽可能基于已存储的 IP 规范化，"
"而不是已存储的主机名"

#. type: TP
#: raw/coreutils/man1/who.1:29
#, no-wrap
msgid "B<-l>, B<--login>"
msgstr "B<-l>, B<--login>"

#. type: Plain text
#: raw/coreutils/man1/who.1:32
msgid "print system login processes"
msgstr "显示系统登录进程"

#. type: TP
#: raw/coreutils/man1/who.1:32
#, no-wrap
msgid "B<--lookup>"
msgstr "B<--lookup>"

#. type: Plain text
#: raw/coreutils/man1/who.1:35
msgid "attempt to canonicalize hostnames via DNS"
msgstr "试图通过 DNS 规范主机名"

#. type: TP
#: raw/coreutils/man1/who.1:35
#, no-wrap
msgid "B<-m>"
msgstr "B<-m>"

#. type: Plain text
#: raw/coreutils/man1/who.1:38
msgid "only hostname and user associated with stdin"
msgstr "仅显示和标准输入关联的主机名和用户"

#. type: TP
#: raw/coreutils/man1/who.1:38
#, no-wrap
msgid "B<-p>, B<--process>"
msgstr "B<-p>, B<--process>"

#. type: Plain text
#: raw/coreutils/man1/who.1:41
msgid "print active processes spawned by init"
msgstr "显示由 init 生成的活跃进程"

#. type: TP
#: raw/coreutils/man1/who.1:41
#, no-wrap
msgid "B<-q>, B<--count>"
msgstr "B<-q>, B<--count>"

#. type: Plain text
#: raw/coreutils/man1/who.1:44
msgid "all login names and number of users logged on"
msgstr "所有登录名称与已登录用户数量"

#. type: TP
#: raw/coreutils/man1/who.1:44
#, no-wrap
msgid "B<-r>, B<--runlevel>"
msgstr "B<-r>, B<--runlevel>"

#. type: Plain text
#: raw/coreutils/man1/who.1:47
msgid "print current runlevel"
msgstr "显示当前运行级别"

#. type: TP
#: raw/coreutils/man1/who.1:47
#, no-wrap
msgid "B<-s>, B<--short>"
msgstr "B<-s>, B<--short>"

# Please make the word "line" clear.
#. type: Plain text
#: raw/coreutils/man1/who.1:50
msgid "print only name, line, and time (default)"
msgstr "仅显示名称、线路与时间（默认）"

#. type: TP
#: raw/coreutils/man1/who.1:50
#, no-wrap
msgid "B<-t>, B<--time>"
msgstr "B<-t>, B<--time>"

#. type: Plain text
#: raw/coreutils/man1/who.1:53
msgid "print last system clock change"
msgstr "显示最近一次系统时钟更改"

#. type: TP
#: raw/coreutils/man1/who.1:53
#, no-wrap
msgid "B<-T>, B<-w>, B<--mesg>"
msgstr "B<-T>, B<-w>, B<--mesg>"

#. type: Plain text
#: raw/coreutils/man1/who.1:56
msgid "add user's message status as +, - or ?"
msgstr "添加用户状态信息，如 +, - 或 ?"

#. type: TP
#: raw/coreutils/man1/who.1:56
#, no-wrap
msgid "B<-u>, B<--users>"
msgstr "B<-u>, B<--users>"

#. type: Plain text
#: raw/coreutils/man1/who.1:59
msgid "list users logged in"
msgstr "w列出已登陆用户"

#. type: TP
#: raw/coreutils/man1/who.1:59
#, no-wrap
msgid "B<--message>"
msgstr "B<--message>"

#. type: Plain text
#: raw/coreutils/man1/who.1:62 raw/coreutils/man1/who.1:65
msgid "same as B<-T>"
msgstr "等价于 B<-T>"

#. type: TP
#: raw/coreutils/man1/who.1:62
#, no-wrap
msgid "B<--writable>"
msgstr "B<--writable>"

#. type: TP
#: raw/coreutils/man1/who.1:65
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/who.1:68
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/who.1:68
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/who.1:71
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: Plain text
#: raw/coreutils/man1/who.1:74
msgid ""
"If FILE is not specified, use I<\\,/var/run/utmp\\/>.  I<\\,/var/log/wtmp\\/"
"> as FILE is common.  If ARG1 ARG2 given, B<-m> presumed: 'am i' or 'mom "
"likes' are usual."
msgstr ""
"如果没有指定文件，默认使用 I<\\,/var/run/utmp\\/>。I<\\,/var/log/wtmp\\/> 常"
"被指定为文件参数。如果给出参数1 和参数2，则等价于 B<-m>：两个参数通常可以是 "
"'am i' 或 'mom likes'。"

