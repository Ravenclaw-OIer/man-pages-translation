#. type: Plain text
#: raw/coreutils/man1/sync.1:5
msgid "sync - Synchronize cached writes to persistent storage"
msgstr "sync - 将缓存内容同步写入持久性存储中"

#. type: SH
#: raw/coreutils/man1/sync.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/sync.1:8
msgid "B<sync> [I<\\,OPTION\\/>] [I<\\,FILE\\/>]..."
msgstr "B<sync> [I<\\,选项\\/>] [I<\\,文件\\/>]..."

#. type: SH
#: raw/coreutils/man1/sync.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/sync.1:12
msgid "Synchronize cached writes to persistent storage"
msgstr "将缓存内容同步写入持久性存储中"

#. type: Plain text
#: raw/coreutils/man1/sync.1:15
msgid ""
"If one or more files are specified, sync only them, or their containing file "
"systems."
msgstr "如果指定了多于一个文件，只同步它们，或者存放它们的文件系统。"

#. type: TP
#: raw/coreutils/man1/sync.1:15
#, no-wrap
msgid "B<-d>, B<--data>"
msgstr "B<-d>, B<--data>"

#. type: Plain text
#: raw/coreutils/man1/sync.1:18
msgid "sync only file data, no unneeded metadata"
msgstr "只同步文件数据，不需要处理元信息"

#. type: TP
#: raw/coreutils/man1/sync.1:18
#, no-wrap
msgid "B<-f>, B<--file-system>"
msgstr "B<-f>, B<--file-system>"

#. type: Plain text
#: raw/coreutils/man1/sync.1:21
msgid "sync the file systems that contain the files"
msgstr "只同步包含这些文件的文件系统"

#. type: TP
#: raw/coreutils/man1/sync.1:21
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/sync.1:24
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/sync.1:24
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/sync.1:27
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: SH
#: raw/coreutils/man1/sync.1:27
#, no-wrap
msgid "BUGS"
msgstr "缺陷"

#. type: Plain text
#: raw/coreutils/man1/sync.1:30
msgid ""
"Persistence guarantees vary per system.  See the system calls below for more "
"details."
msgstr "持久性保证因系统而异。请查阅相关系统调用以了解详情。"

