msgstr "即 B<-t> dl，指定十进制整数对照输出格式"

#. type: TP
#: raw/coreutils/man1/od.1:86
#, no-wrap
msgid "B<-l>"
msgstr "B<-l>"

#. type: Plain text
#: raw/coreutils/man1/od.1:89
msgid "same as B<-t> dL, select decimal longs"
msgstr "即 B<-t> dL，指定十进制长整数对照输出格式"

#. type: TP
#: raw/coreutils/man1/od.1:89
#, no-wrap
msgid "B<-o>"
msgstr "B<-o>"

#. type: Plain text
#: raw/coreutils/man1/od.1:92
