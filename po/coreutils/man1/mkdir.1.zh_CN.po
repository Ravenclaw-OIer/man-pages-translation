#. type: TH
#: raw/coreutils/man1/mkdir.1:2
#, no-wrap
msgid "MKDIR"
msgstr "MKDIR"

#. type: TH
#: raw/coreutils/man1/mkdir.1:2
#, no-wrap
msgid "September 2022"
msgstr "2022年9月"

#. type: TH
#: raw/coreutils/man1/mkdir.1:2
#, no-wrap
#| msgid "GNU coreutils 8.32"
msgid "GNU coreutils 9.1"
msgstr "GNU coreutils 9.1"

#. type: TH
#: raw/coreutils/man1/mkdir.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: raw/coreutils/man1/mkdir.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:5
msgid "mkdir - make directories"
msgstr "mkdir - 创建目录"

#. type: SH
#: raw/coreutils/man1/mkdir.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:8
msgid "B<mkdir> [I<\\,OPTION\\/>]... I<\\,DIRECTORY\\/>..."
msgstr "B<mkdir> [I<\\,选项\\/>]... I<\\,目录列表\\/>..."

#. type: SH
#: raw/coreutils/man1/mkdir.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:12
msgid "Create the DIRECTORY(ies), if they do not already exist."
msgstr "如果目录列表中的目录不存在，则创建之。"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "必选参数对长短选项同时适用。"

#. type: TP
#: raw/coreutils/man1/mkdir.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<\\,MODE\\/>"
msgstr "B<-m>, B<--mode>=I<\\,模式\\/>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:17
msgid "set file mode (as in chmod), not a=rwx - umask"
msgstr "设置文件模式(类似chmod)，而不是 rwx 减 umask"

#. type: TP
#: raw/coreutils/man1/mkdir.1:17
#, no-wrap
msgid "B<-p>, B<--parents>"
msgstr "B<-p>, B<--parents>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:21
#| msgid "no error if existing, make parent directories as needed"
msgid ""
"no error if existing, make parent directories as needed, with their file "
"modes unaffected by any B<-m> option."
msgstr "需要时创建目标目录的父目录，但即使这些目录已存在也不视为错误，且其文件模式也不受 B<-m> 选项影响。"

#. type: TP
#: raw/coreutils/man1/mkdir.1:21
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:24
msgid "print a message for each created directory"
msgstr "为每一个已创建的目录打印信息"

#. type: TP
#: raw/coreutils/man1/mkdir.1:24
#, no-wrap
msgid "B<-Z>"
msgstr "B<-Z>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:28
msgid ""
"set SELinux security context of each created directory to the default type"
msgstr "将每个创建的目录的 SELinux 安全上下文设置为默认类型"

#. type: TP
#: raw/coreutils/man1/mkdir.1:28
#, no-wrap
msgid "B<--context>[=I<\\,CTX\\/>]"
msgstr "B<--context>[=I<\\,CTX\\/>]"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:32
msgid ""
"like B<-Z>, or if CTX is specified then set the SELinux or SMACK security "
"context to CTX"
msgstr ""
"类似 B<-Z>，或者如果指定了上下文，则将 SELinux 或者 SMACK 安全上下文设置为指"
"定类型"

#. type: TP
#: raw/coreutils/man1/mkdir.1:32
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:35
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/mkdir.1:35
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:38
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: SH
#: raw/coreutils/man1/mkdir.1:38
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:40
msgid "Written by David MacKenzie."
msgstr "由 David MacKenzie 编写。"

#. type: SH
#: raw/coreutils/man1/mkdir.1:40
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:42
msgid ""
"GNU coreutils online help: E<lt>https://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 的在线帮助： E<lt>https://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: raw/coreutils/man1/mkdir.1:44
msgid ""
"Report any translation bugs to E<lt>https://translationproject.org/team/E<gt>"
msgstr "请向 E<lt>https://translationproject.org/team/zh_CN.htmlE<gt> 报告翻译错误。"

