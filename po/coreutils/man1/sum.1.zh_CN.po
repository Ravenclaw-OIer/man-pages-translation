msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man1/sum.1:5
msgid "sum - checksum and count the blocks in a file"
msgstr "sum - 求校验和并统计文件块数目"

#. type: SH
#: raw/coreutils/man1/sum.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/sum.1:8
msgid "B<sum> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<sum> [I<\\,选项\\/>]... [I<\\,文件列表\\/>]..."

#. type: SH
#: raw/coreutils/man1/sum.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/sum.1:12
msgid "Print or check BSD (16-bit) checksums."
msgstr "打印或检查 BSD（16位）校验和。"

#. type: Plain text
#: raw/coreutils/man1/sum.1:14
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "如果没有指定文件，或者指定文件为“-”，则从标准输入读取。"

#. type: TP
#: raw/coreutils/man1/sum.1:14
#, no-wrap
msgid "B<-r>"
msgstr "B<-r>"

#. type: Plain text
#: raw/coreutils/man1/sum.1:17
#| msgid "use BSD sum algorithm, use 1K blocks"
msgid "use BSD sum algorithm (the default), use 1K blocks"
msgstr "使用 BSD 求和算法（默认行为），使用 1K 文件块"

#. type: TP
#: raw/coreutils/man1/sum.1:17
#, no-wrap
msgid "B<-s>, B<--sysv>"
msgstr "B<-s>, B<--sysv>"

#. type: Plain text
#: raw/coreutils/man1/sum.1:20
msgid "use System V sum algorithm, use 512 bytes blocks"
msgstr "使用 System V 求和算法，使用 512 字节文件块"

#. type: TP
#: raw/coreutils/man1/sum.1:20
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/sum.1:23
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/sum.1:23
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/sum.1:26
msgid "output version information and exit"
msgstr "显示版本信息并退出"

