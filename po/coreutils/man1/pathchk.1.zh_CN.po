msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:12
msgid "Diagnose invalid or unportable file names."
msgstr "诊断不合法或没有移植性的文件名。"

#. type: TP
#: raw/coreutils/man1/pathchk.1:12
#, no-wrap
msgid "B<-p>"
msgstr "B<-p>"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:15
msgid "check for most POSIX systems"
msgstr "针对大多数 POSIX 系统进行检查"

#. type: TP
#: raw/coreutils/man1/pathchk.1:15
#, no-wrap
msgid "B<-P>"
msgstr "B<-P>"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:18
msgid "check for empty names and leading \"-\""
msgstr "检查空文件名和起始的“-”"

#. type: TP
#: raw/coreutils/man1/pathchk.1:18
#, no-wrap
msgid "B<--portability>"
msgstr "B<--portability>"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:21
msgid "check for all POSIX systems (equivalent to B<-p> B<-P>)"
msgstr "针对所有 POSIX 系统进行检查（等价于 B<-p> B<-P>）"

#. type: TP
#: raw/coreutils/man1/pathchk.1:21
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:24
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/pathchk.1:24
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/pathchk.1:27
msgid "output version information and exit"
