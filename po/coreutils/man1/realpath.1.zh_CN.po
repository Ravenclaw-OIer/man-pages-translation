#. type: TH
#: raw/coreutils/man1/realpath.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: raw/coreutils/man1/realpath.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:5
msgid "realpath - print the resolved path"
msgstr "realpath - 输出经过解析的文件路径"

#. type: SH
#: raw/coreutils/man1/realpath.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:8
msgid "B<realpath> [I<\\,OPTION\\/>]... I<\\,FILE\\/>..."
msgstr "B<realpath> [I<\\,选项\\/>]... I<\\,文件列表\\/>..."

#. type: SH
#: raw/coreutils/man1/realpath.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:13
msgid ""
"Print the resolved absolute file name; all but the last component must exist"
msgstr "输出经过解析的绝对文件名；文件名除最后部分以外的所有部分都必须存在"

#. type: TP
#: raw/coreutils/man1/realpath.1:13
#, no-wrap
msgid "B<-e>, B<--canonicalize-existing>"
msgstr "B<-e>, B<--canonicalize-existing>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:16
msgid "all components of the path must exist"
msgstr "路径中的所有组成部分必须存在"

#. type: TP
#: raw/coreutils/man1/realpath.1:16
#, no-wrap
msgid "B<-m>, B<--canonicalize-missing>"
msgstr "B<-m>, B<--canonicalize-missing>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:19
msgid "no path components need exist or be a directory"
msgstr "路径的各个部分不必须存在，不存在者将被视为目录"

#. type: TP
#: raw/coreutils/man1/realpath.1:19
#, no-wrap
msgid "B<-L>, B<--logical>"
msgstr "B<-L>, B<--logical>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:22
msgid "resolve '..' components before symlinks"
msgstr "在符号链接前解析“..”部分"

#. type: TP
#: raw/coreutils/man1/realpath.1:22
#, no-wrap
msgid "B<-P>, B<--physical>"
msgstr "B<-P>, B<--physical>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:25
msgid "resolve symlinks as encountered (default)"
msgstr "解析遇到的符号链接（默认）"

#. type: TP
#: raw/coreutils/man1/realpath.1:25
#, no-wrap
msgid "B<-q>, B<--quiet>"
msgstr "B<-q>, B<--quiet>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:28
msgid "suppress most error messages"
msgstr "隐藏大多数错误信息"

#. type: TP
#: raw/coreutils/man1/realpath.1:28
#, no-wrap
msgid "B<--relative-to>=I<\\,DIR\\/>"
msgstr "B<--relative-to>=I<\\,指定目录\\/>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:31
msgid "print the resolved path relative to DIR"
msgstr "输出相对于指定目录所解析的路径"

#. type: TP
#: raw/coreutils/man1/realpath.1:31
#, no-wrap
msgid "B<--relative-base>=I<\\,DIR\\/>"
msgstr "B<--relative-base>=I<\\,指定目录\\/>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:34
msgid "print absolute paths unless paths below DIR"
msgstr "路径在指定目录之下则输出相对它的路径，否则输出绝对路径"

#. type: TP
#: raw/coreutils/man1/realpath.1:34
#, no-wrap
msgid "B<-s>, B<--strip>, B<--no-symlinks>"
msgstr "B<-s>, B<--strip>, B<--no-symlinks>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:37
msgid "don't expand symlinks"
msgstr "不要扩展符号链接"

#. type: TP
#: raw/coreutils/man1/realpath.1:37
#, no-wrap
msgid "B<-z>, B<--zero>"
msgstr "B<-z>, B<--zero>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:40
msgid "end each output line with NUL, not newline"
msgstr "使每行输出以 NUL 字符结束，不添加换行符"

#. type: TP
#: raw/coreutils/man1/realpath.1:40
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:43
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/realpath.1:43
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/realpath.1:46
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: SH
