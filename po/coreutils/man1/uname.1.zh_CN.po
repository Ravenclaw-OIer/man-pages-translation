msgid "UNAME"
msgstr "UNAME"

#. type: TH
#: raw/coreutils/man1/uname.1:2
#, no-wrap
msgid "September 2022"
msgstr "2022年9月"

#. type: TH
#: raw/coreutils/man1/uname.1:2
#, no-wrap
#| msgid "GNU coreutils 8.32"
msgid "GNU coreutils 9.1"
msgstr "GNU coreutils 9.1"

#. type: TH
#: raw/coreutils/man1/uname.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: raw/coreutils/man1/uname.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man1/uname.1:5
msgid "uname - print system information"
msgstr "uname - 输出系统信息"

#. type: SH
#: raw/coreutils/man1/uname.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man1/uname.1:8
msgid "B<uname> [I<\\,OPTION\\/>]..."
msgstr "B<uname> [I<\\,选项\\/>]..."

#. type: SH
#: raw/coreutils/man1/uname.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man1/uname.1:12
msgid "Print certain system information.  With no OPTION, same as B<-s>."
msgstr "输出特定的系统信息。如果没有给定选项，则与 B<-s> 相同。"

#. type: TP
#: raw/coreutils/man1/uname.1:12
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:16
msgid ""
"print all information, in the following order, except omit B<-p> and B<-i> "
"if unknown:"
msgstr ""
"按照如下所述的顺序输出全部信息，在信息未知的情况下会忽略 B<-p> 和 B<-i> 选"
"项："

#. type: TP
#: raw/coreutils/man1/uname.1:16
#, no-wrap
msgid "B<-s>, B<--kernel-name>"
msgstr "B<-s>, B<--kernel-name>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:19
msgid "print the kernel name"
msgstr "输出内核名称"

#. type: TP
#: raw/coreutils/man1/uname.1:19
#, no-wrap
msgid "B<-n>, B<--nodename>"
msgstr "B<-n>, B<--nodename>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:22
msgid "print the network node hostname"
msgstr "输出网络节点主机名"

#. type: TP
#: raw/coreutils/man1/uname.1:22
#, no-wrap
msgid "B<-r>, B<--kernel-release>"
msgstr "B<-r>, B<--kernel-release>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:25
msgid "print the kernel release"
msgstr "输出内核发行版本"

#. type: TP
#: raw/coreutils/man1/uname.1:25
#, no-wrap
msgid "B<-v>, B<--kernel-version>"
msgstr "B<-v>, B<--kernel-version>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:28
msgid "print the kernel version"
msgstr "输出内核版本信息"

#. type: TP
#: raw/coreutils/man1/uname.1:28
#, no-wrap
msgid "B<-m>, B<--machine>"
msgstr "B<-m>, B<--machine>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:31
msgid "print the machine hardware name"
msgstr "输出机器硬件名称"

#. type: TP
#: raw/coreutils/man1/uname.1:31
#, no-wrap
msgid "B<-p>, B<--processor>"
msgstr "B<-p>, B<--processor>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:34
msgid "print the processor type (non-portable)"
msgstr "输出处理器类型（不可移植）"

#. type: TP
#: raw/coreutils/man1/uname.1:34
#, no-wrap
msgid "B<-i>, B<--hardware-platform>"
msgstr "B<-i>, B<--hardware-platform>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:37
msgid "print the hardware platform (non-portable)"
msgstr "输出硬件平台信息（不可移植）"

#. type: TP
#: raw/coreutils/man1/uname.1:37
#, no-wrap
msgid "B<-o>, B<--operating-system>"
msgstr "B<-o>, B<--operating-system>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:40
msgid "print the operating system"
msgstr "输出操作系统信息"

#. type: TP
#: raw/coreutils/man1/uname.1:40
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:43
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man1/uname.1:43
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:46
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: SH
#: raw/coreutils/man1/uname.1:46
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: raw/coreutils/man1/uname.1:48
msgid "Written by David MacKenzie."
msgstr "由 David MacKenzie 编写。"

#. type: SH
#: raw/coreutils/man1/uname.1:48
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: raw/coreutils/man1/uname.1:50
msgid ""
"GNU coreutils online help: E<lt>https://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 的在线帮助： E<lt>https://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: raw/coreutils/man1/uname.1:52
msgid ""
"Report any translation bugs to E<lt>https://translationproject.org/team/E<gt>"
msgstr "请向 E<lt>https://translationproject.org/team/zh_CN.htmlE<gt> 报告翻译错误。"

