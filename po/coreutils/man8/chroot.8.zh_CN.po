#. type: SH
#: raw/coreutils/man8/chroot.8:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:5
msgid "chroot - run command or interactive shell with special root directory"
msgstr "chroot - 在特殊的根目录下运行目录或者交互式 shell"

#. type: SH
#: raw/coreutils/man8/chroot.8:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:8
msgid ""
"B<chroot> [I<\\,OPTION\\/>] I<\\,NEWROOT \\/>[I<\\,COMMAND \\/>[I<\\,ARG\\/"
">]...]"
msgstr "B<chroot> [I<\\,选项\\/>] I<\\,新根 \\/>[I<\\,命令 \\/>[I<\\,参数\\/>]...]"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:11
msgid "B<chroot> I<\\,OPTION\\/>"
msgstr "B<chroot> I<\\,选项\\/>"

#. type: SH
#: raw/coreutils/man8/chroot.8:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:15
msgid "Run COMMAND with root directory set to NEWROOT."
msgstr "以指定的新根为运行指定命令时的的根目录。"

#. type: TP
#: raw/coreutils/man8/chroot.8:15
#, no-wrap
msgid "B<--groups>=I<\\,G_LIST\\/>"
msgstr "B<--groups>=I<\\,G_LIST\\/>"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:18
msgid "specify supplementary groups as g1,g2,..,gN"
msgstr "以 g1,g2,...,gN 的形式指定附加用户组"

#. type: TP
#: raw/coreutils/man8/chroot.8:18
#, no-wrap
msgid "B<--userspec>=I<\\,USER\\/>:GROUP"
msgstr "B<--userspec>=I<\\,USER\\/>:GROUP"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:21
msgid "specify user and group (ID or name) to use"
msgstr "指定要使用的用户和用户组（ID 或者名称）"

#. type: TP
#: raw/coreutils/man8/chroot.8:21
#, no-wrap
msgid "B<--skip-chdir>"
msgstr "B<--skip-chdir>"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:24
msgid "do not change working directory to '/'"
msgstr "不要将工作目录切换为“/”"

#. type: TP
#: raw/coreutils/man8/chroot.8:24
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:27
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/coreutils/man8/chroot.8:27
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:30
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: Plain text
#: raw/coreutils/man8/chroot.8:32
msgid ""
"If no command is given, run '\"$SHELL\" B<-i>' (default: '/bin/sh B<-i>')."
msgstr "如果没有给定命令，则运行“\"$SHELL\" B<-i>”（默认为：“/bin/sh B<-i>”）。"

